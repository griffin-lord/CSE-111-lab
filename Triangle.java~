public class Triangle{
  public int a;
  public int b;
  public int c;
   
    public Triangle(int a1, int b1,nint c1){
    a=a1;
    b=b1; 
    c=c1;
  }
  
     public Triangle(int a2, int b2,nint c2){
    a=a2;
    b=b2;
    c=c2;
  }
  
    public Triangle(int a3, int b3,nint c3){
    a=a3;
    b=b3; 
    c=c3; 
  }
    public Triangle(int a4, int b4,nint c4){
    a=a4;
    b=b4; 
    c=c4; 
  }
    
    public String printTriangleType(){
      if (a=b=c){
        return "This is an Equilateral Triangle.";
      }
      else if( a=b || b=c || a=c){
        return "This is an Isosceles Triangle.";
    }
      else{
        return "This is an Scalene Triangle.";
      }